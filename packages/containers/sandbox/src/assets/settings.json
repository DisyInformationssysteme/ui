{
  "actions": {
    "menu:headerBar-demo": {
      "id": "menu:headerBar-demo",
      "label": "HeaderBar",
      "icon": "talend-arrow-right",
      "payload": {
        "type": "MENU_LINK",
        "cmf": {
          "routerReplace": "/headerbar-demo"
        }
      }
    },
    "component-form": {
      "icon": "talend-burger",
      "label": "ComponentForm",
      "onClickDispatch": {
        "type": "MENU_COMPONENT_FORM_CLICKED",
        "cmf": {
          "routerPush": "/ComponentForm"
        }
      }
    }
  },
  "props": {
    "RedirectContainer#default": {
      "path": "/home"
    },
    "HeaderBar#default": {
      "logo": {
        "icon": "talend-logo-square",
        "isFull": false
      },
      "brand": {
        "label": "Talend containers showcase"
      }
    },
    "SidePanel#default": {
      "actionIds": [
        "menu:headerBar-demo",
        "component-form"
      ]
    },
    "Showcase#default": {
      "content": {
        "component": "EmptyShowcase"
      }
    },
    "Showcase#headerBar-demo": {
      "content": {
        "component": "HeaderBar",
        "logo": {
          "id": "header-bar-demo-logo",
          "icon": "talend-world",
          "isFull": false
        },
        "brand": {
          "id": "header-bar-demo-brand",
          "label": "Choose a product"
        },
        "products": {
          "id": "header-bar-demo-brands"
        },
        "productsUrl": "/api/mock/header-bar/products-list",
        "productsLang": "en-EN"
      }
    },
    "Layout#example": {
      "mode": "TwoColumns",
      "header": [{ "component": "HeaderBar" }],
      "one": [{ "component": "SidePanel" }],
      "content": [{ "component": "ComponentForm", "componentId": "example" }]
    },
    "ComponentForm#example": {
      "definitionURL": "/api/v1/forms/example",
      "uiSpecPath": "ui",
      "triggerURL": "/api/v1/application/action",
      "className": "well"
    }
  },
  "routes": {
    "path": "/",
    "component": "App",
    "indexRoute": {
      "component": "Redirect"
    },
    "childRoutes": [
      {
        "path": "home",
        "component": "Showcase"
      },
      {
        "path": "headerbar-demo",
        "component": "Showcase",
        "componentId": "headerBar-demo"
      },
      {
        "path": "ComponentForm",
        "component": "Layout",
        "componentId": "example"
      },
      {
        "path": "*",
        "component": "Redirect"
      }
    ]
  }
}
