  "homepage": "https://github.com/Talend/react-ui-abstraction#readme",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "6.1.2",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
