{
  "name": "react-cmf-bootstrap",
  "description": "Provide base widgets for react-cmf",
  "main": "lib/index.js",
  "scripts": {
    "docs:js": "jsdoc -c docs.json",
    "docs:cleanup": "rm -rf docs/jsodc/*",
    "docs": "npm run docs:cleanup && npm run docs:js",
    "prepublish": "babel -d lib ./src/ && rm lib/**/*.test.js && cpx './src/**/*.scss' ./lib",
    "start": "npm run storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint:es": "eslint --config .eslintrc src/*.js",
    "lint": "npm run lint:es",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "watch": "npm-watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs/storybook"
  },
  "watch": {
    "prepublish": "src/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/react-cmf-bootstrap.git"
  },
  "keywords": [
    "react",
    "bootstrap"
  ],
  "author": "Jean-Michel FRANCOIS <jmfrancois@talend.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Talend/react-cmf-bootstrap/issues"
  },
  "homepage": "https://github.com/Talend/react-cmf-bootstrap#readme",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "@kadira/storybook": "^2.19.0",
    "babel-cli": "6.14.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "classnames": "2",
    "cpx": "^1.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "immutable": "3",
    "invariant": "2",
    "jest-cli": "^15.1.1",
    "jsdoc": "^3.4.1",
    "node-sass": "^3.10.0",
    "react": "15",
    "react-addons-test-utils": "^15.3.0",
    "react-bootstrap": "0",
    "react-css-themr": "^1.3.0",
    "react-cmf": "0",
    "react-dom": "15",
    "react-i18next": "^1.8.0",
    "react-redux": "4",
    "react-router": "2",
    "react-router-redux": "4",
    "react-storybook-cmf": "^0",
    "react-stub-context": "^0.7.0",
    "react-test-renderer": "15",
    "redux": "3",
    "redux-thunk": "2",
    "sass-loader": "^4.0.2",
    "semantic-release": "^4.3.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testRegex": "src/.*\\.test\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/styleMock.js"
    }
  },
  "peerDependencies": {
    "react-cmf": "<1",
    "react-bootstrap": "0",
    "react-css-themr": "^1.3.0"
  },
  "dependencies": {}
}
