{
  "name": "react-ui-abstraction",
  "description": "A framework built on top of best react libraries",
  "main": "lib/react-ui-abstraction.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/jest",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:cov": "./node_modules/.bin/jest --coverage",
    "lint:es": "./node_modules/.bin/eslint --config .eslintrc src/*.js",
    "lint": "npm run lint:es",
    "semantic-release": "./node_modules/.bin/semantic-release pre && npm publish && ./node_modules/.bin/semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmfrancois/react-ui-abstraction.git"
  },
  "keywords": [
    "react",
    "framework"
  ],
  "author": "Jean-Michel FRANCOIS <jmfrancois@talend.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/talend/react-ui-abstraction/issues"
  },
  "homepage": "https://github.com/talend/react-ui-abstraction#readme",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "6.1.0",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "classnames": "^2.2.5",
    "enzyme": "^2.4.1",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-react": "5.2.2",
    "jest-cli": "^14.1.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-test-renderer": "^15.3.0",
    "redux": "^3.5.2",
    "semantic-release": "^4.3.5",
    "webpack": "^1.13.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "automock": false
  },
  "peerDependencies": {
    "classnames": "^2.2.5",
    "react": "^0.14.0 || ^15.3.0",
    "react-dom": "^0.14.0 || ^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2"
  }
}
