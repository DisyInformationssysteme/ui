{
    "name": "bootstrap-tlnd-theme",
    "description": "Bootstrap theme based on Talend styleguide",
    "main": "dist/bootstrap.js",
    "engines": {
        "npm": ">=2.7.1"
    },
    "scripts": {
        "semantic-release": "semantic-release pre && npm publish && semantic-release post",
        "prepublish": "webpack",
        "start": "webpack-dev-server",
        "test": "babel -d .tmp/test test/ && casperjs test .tmp/test/screenshots.spec.js",
        "test:rebase": "babel -d .tmp/test test/ && casperjs test .tmp/test/screenshots.spec.js --rebase"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Talend/bootstrap-theme.git"
    },
    "keywords": [
        "bootstrap",
        "theme"
    ],
    "author": "Talend",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/Talend/bootstrap-theme/issues"
    },
    "homepage": "https://github.com/Talend/bootstrap-theme#readme",
    "devDependencies": {
        "babel-cli": "^6.14.0",
        "babel-preset-es2015": "^6.14.0",
        "css-loader": "^0.23.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.9.0",
        "less": "^2.7.1",
        "less-loader": "^2.2.3",
        "node-sass": "^3.8.0",
        "path": "^0.12.7",
        "phantomcss": "^1.1.3",
        "sass-loader": "^4.0.0",
        "semantic-release": "4.3.5",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "webpack": "^1.13.1",
        "webpack-dev-server": "^1.14.1"
    },
    "dependencies": {
        "bootstrap-sass": "^3.3.7",
        "inconsolata": "0.0.2",
        "source-sans-pro": "^2.0.10"
    }
}
